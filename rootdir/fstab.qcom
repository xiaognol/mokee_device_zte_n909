# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>                                 <mnt_point>       <type>  <mnt_flags and options>                 <fs_mgr_flags>
/dev/block/platform/msm_sdcc.3/by-num/p8                  /boot             emmc    defaults                                defaults
/dev/block/platform/msm_sdcc.3/by-num/p16                 /recovery         emmc    defaults                                defaults
/dev/block/platform/msm_sdcc.3/by-num/p15                 /cache            ext4    nosuid,nodev,barrier=1                  wait,check
/dev/block/platform/msm_sdcc.3/by-num/p12                 /system           ext4    ro,barrier=1                            wait
/dev/block/platform/msm_sdcc.3/by-num/p13                 /data             ext4    nosuid,nodev,noauto_da_alloc,barrier=1  wait,check,encryptable=footer,length=-16384
/devices/platform/msm_sdcc.1/mmc_host                     auto              auto    defaults                                voldmanaged=sdcard1:auto,noemulatedsd
/devices/platform/msm_sdcc.3/mmc_host/mmc0                auto              auto    defaults                                voldmanaged=sdcard0:20,nonremovable,noemulatedsd
